---
import Footer from "../components/Footer.astro";


interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html class="flex flex-auto justify-center items-center" lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Pagina de Anubis, una gata" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="icons.png" />
		<title>{title}</title>
	</head>
	<body class="bg-[#121212]">
		<slot />
		<Footer />
	</body>
</html>
